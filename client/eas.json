{
  "cli": {
    "version": ">= 14.7.1",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "android": {
        "developmentClient": true,
        "distribution": "internal",
        "buildType": "apk"
      },
      "env": {
        "API_KEY": "@env:API_KEY",
        "APP_ID": "@env:APP_ID",
        "AUTH_DOMAIN": "@env:AUTH_DOMAIN",
        "MESSAGING_SENDER_ID": "@env:MESSAGING_SENDER_ID",
        "PROJECT_ID": "@env:PROJECT_ID",
        "STORAGE_BUCKET": "@env:STORAGE_BUCKET"
      }
    },
    "preview": {
      "android": {
        "distribution": "internal"
      },
      "env": {
        "API_KEY": "@env:API_KEY",
        "APP_ID": "@env:APP_ID",
        "AUTH_DOMAIN": "@env:AUTH_DOMAIN",
        "MESSAGING_SENDER_ID": "@env:MESSAGING_SENDER_ID",
        "PROJECT_ID": "@env:PROJECT_ID",
        "STORAGE_BUCKET": "@env:STORAGE_BUCKET"
      }
    },
    "production": {
      "android": {
        "autoIncrement": true
      },
      "env": {
        "API_KEY": "@env:API_KEY",
        "APP_ID": "@env:APP_ID",
        "AUTH_DOMAIN": "@env:AUTH_DOMAIN",
        "MESSAGING_SENDER_ID": "@env:MESSAGING_SENDER_ID",
        "PROJECT_ID": "@env:PROJECT_ID",
        "STORAGE_BUCKET": "@env:STORAGE_BUCKET"
      }
    }
  },
  "submit": {
    "production": {}
  }
}
